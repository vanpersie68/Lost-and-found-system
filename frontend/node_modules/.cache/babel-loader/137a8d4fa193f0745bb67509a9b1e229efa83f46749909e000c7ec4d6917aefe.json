{"ast":null,"code":"export default {\n  name: \"sidebarMenu\",\n  data() {\n    return {\n      singleOpenName: []\n    };\n  },\n  props: {\n    menuList: Array,\n    iconSize: Number,\n    menuTheme: {\n      type: String,\n      default: \"dark\"\n    },\n    openNames: {\n      type: Array\n    }\n  },\n  methods: {\n    changeMenu(active) {\n      this.$emit(\"on-change\", active);\n    },\n    handleSelect(name) {\n      this.$emit(\"on-select\", name);\n    },\n    itemTitle(item) {\n      return item.title;\n    },\n    getOpenedNamesByActiveName(name) {\n      return this.$route.matched.map(item => item.name).filter(item => item !== name);\n    }\n  },\n  updated() {\n    this.$nextTick(() => {\n      if (this.$refs.sideMenu) {\n        this.$refs.sideMenu.updateOpened();\n      }\n    });\n  },\n  watch: {\n    // 监听路由变化\n    $route(to, from) {\n      this.singleOpenName = [this.$route.matched[0].name];\n    }\n  },\n  mounted() {\n    this.singleOpenName = [this.$route.matched[0].name];\n  }\n};","map":{"version":3,"names":["name","data","singleOpenName","props","menuList","Array","iconSize","Number","menuTheme","type","String","default","openNames","methods","changeMenu","active","$emit","handleSelect","itemTitle","item","title","getOpenedNamesByActiveName","$route","matched","map","filter","updated","$nextTick","$refs","sideMenu","updateOpened","watch","to","from","mounted"],"sources":["src/views/main-components/shrinkable-menu/components/sidebarMenu.vue"],"sourcesContent":["<style lang=\"less\">\n@import \"../styles/menu.less\";\n</style>\n<template>\n  <Menu\n    ref=\"sideMenu\"\n    accordion\n    :active-name=\"$route.name\"\n    :open-names=\"singleOpenName\"\n    :theme=\"menuTheme\"\n    width=\"auto\"\n    @on-select=\"changeMenu\"\n  >\n    <template v-for=\"item in menuList\">\n      <template>\n        <Submenu :name=\"item.name\" :key=\"item.name\">\n          <template slot=\"title\">\n            <Icon :type=\"item.icon\" :size=\"iconSize\"></Icon>\n            <span class=\"layout-text\">{{ itemTitle(item) }}</span>\n          </template>\n          <template v-for=\"child in item.children\">\n            <MenuItem :name=\"child.name\" :key=\"'menuitem' + child.name\">\n              <Icon :type=\"child.icon\" :size=\"iconSize\" :key=\"'icon' + child.name\"></Icon>\n              <span class=\"layout-text\" :key=\"'title' + child.name\">{{ itemTitle(child) }}</span>\n            </MenuItem>\n          </template>\n        </Submenu>\n      </template>\n    </template>\n  </Menu>\n</template>\n\n<script>\nexport default {\n  name: \"sidebarMenu\",\n  data() {\n    return {\n      singleOpenName: []\n    };\n  },\n  props: {\n    menuList: Array,\n    iconSize: Number,\n    menuTheme: {\n      type: String,\n      default: \"dark\"\n    },\n    openNames: {\n      type: Array\n    }\n  },\n  methods: {\n    changeMenu(active) {\n      this.$emit(\"on-change\", active);\n    },\n    handleSelect(name) {\n      this.$emit(\"on-select\", name);\n    },\n    itemTitle(item) {\n      return item.title;\n    },\n    getOpenedNamesByActiveName(name) {\n      return this.$route.matched\n        .map(item => item.name)\n        .filter(item => item !== name);\n    }\n  },\n  updated() {\n    this.$nextTick(() => {\n      if (this.$refs.sideMenu) {\n        this.$refs.sideMenu.updateOpened();\n      }\n    });\n  },\n  watch: {\n    // 监听路由变化\n    $route(to, from) {\n      this.singleOpenName = [this.$route.matched[0].name];\n    }\n  },\n  mounted() {\n    this.singleOpenName = [this.$route.matched[0].name];\n  }\n};\n</script>\n"],"mappings":"AAiCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,cAAA;IACA;EACA;EACAC,KAAA;IACAC,QAAA,EAAAC,KAAA;IACAC,QAAA,EAAAC,MAAA;IACAC,SAAA;MACAC,IAAA,EAAAC,MAAA;MACAC,OAAA;IACA;IACAC,SAAA;MACAH,IAAA,EAAAJ;IACA;EACA;EACAQ,OAAA;IACAC,WAAAC,MAAA;MACA,KAAAC,KAAA,cAAAD,MAAA;IACA;IACAE,aAAAjB,IAAA;MACA,KAAAgB,KAAA,cAAAhB,IAAA;IACA;IACAkB,UAAAC,IAAA;MACA,OAAAA,IAAA,CAAAC,KAAA;IACA;IACAC,2BAAArB,