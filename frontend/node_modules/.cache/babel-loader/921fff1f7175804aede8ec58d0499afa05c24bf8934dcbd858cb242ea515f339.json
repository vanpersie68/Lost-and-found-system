{"ast":null,"code":"import sidebarMenu from \"./components/sidebarMenu.vue\";\nimport sidebarMenuShrink from \"./components/sidebarMenuShrink.vue\";\nimport util from \"@/libs/util\";\nexport default {\n  name: \"shrinkableMenu\",\n  components: {\n    sidebarMenu,\n    sidebarMenuShrink\n  },\n  props: {\n    shrink: {\n      type: Boolean,\n      default: false\n    },\n    menuList: {\n      type: Array,\n      required: true\n    },\n    theme: {\n      type: String,\n      default: \"dark\",\n      validator(val) {\n        return util.oneOf(val, [\"dark\", \"light\"]);\n      }\n    },\n    beforePush: {\n      type: Function\n    },\n    openNames: {\n      type: Array\n    }\n  },\n  computed: {\n    bgColor() {\n      return \"#323232\";\n    },\n    shrinkIconColor() {\n      return this.theme == \"dark\" ? \"#fff\" : \"#515a6e\";\n    }\n  },\n  methods: {\n    handleChange(name) {\n      let willpush = true;\n      if (this.beforePush !== undefined) {\n        if (!this.beforePush(name)) {\n          willpush = false;\n        }\n      }\n      if (willpush) {\n        this.$router.push({\n          name: name\n        });\n      }\n      this.$emit(\"on-change\", name);\n    }\n  }\n};","map":{"version":3,"names":["sidebarMenu","sidebarMenuShrink","util","name","components","props","shrink","type","Boolean","default","menuList","Array","required","theme","String","validator","val","oneOf","beforePush","Function","openNames","computed","bgColor","shrinkIconColor","methods","handleChange","willpush","undefined","$router","push","$emit"],"sources":["src/views/main-components/shrinkable-menu/shrinkable-menu.vue"],"sourcesContent":["<style lang=\"less\">\n@import \"./styles/menu.less\";\n</style>\n\n<template>\n  <div :style=\"{background: bgColor}\" class=\"ivu-shrinkable-menu\">\n    <slot name=\"top\"></slot>\n    <sidebar-menu\n      v-show=\"!shrink\"\n      :menu-theme=\"theme\"\n      :menu-list=\"menuList\"\n      :open-names=\"openNames\"\n      @on-change=\"handleChange\"\n    ></sidebar-menu>\n    <sidebar-menu-shrink\n      v-show=\"shrink\"\n      :menu-theme=\"theme\"\n      :menu-list=\"menuList\"\n      :icon-color=\"shrinkIconColor\"\n      @on-change=\"handleChange\"\n    ></sidebar-menu-shrink>\n  </div>\n</template>\n\n<script>\nimport sidebarMenu from \"./components/sidebarMenu.vue\";\nimport sidebarMenuShrink from \"./components/sidebarMenuShrink.vue\";\nimport util from \"@/libs/util\";\nexport default {\n  name: \"shrinkableMenu\",\n  components: {\n    sidebarMenu,\n    sidebarMenuShrink\n  },\n  props: {\n    shrink: {\n      type: Boolean,\n      default: false\n    },\n    menuList: {\n      type: Array,\n      required: true\n    },\n    theme: {\n      type: String,\n      default: \"dark\",\n      validator(val) {\n        return util.oneOf(val, [\"dark\", \"light\"]);\n      }\n    },\n    beforePush: {\n      type: Function\n    },\n    openNames: {\n      type: Array\n    }\n  },\n  computed: {\n    bgColor() {\n      return \"#323232\";\n    },\n    shrinkIconColor() {\n      return this.theme == \"dark\" ? \"#fff\" : \"#515a6e\";\n    }\n  },\n  methods: {\n    handleChange(name) {\n      let willpush = true;\n      if (this.beforePush !== undefined) {\n        if (!this.beforePush(name)) {\n          willpush = false;\n        }\n      }\n      if (willpush) {\n        this.$router.push({\n          name: name\n        });\n      }\n      this.$emit(\"on-change\", name);\n    }\n  }\n};\n</script>\n"],"mappings":"AAyBA,OAAAA,WAAA;AACA,OAAAC,iBAAA;AACA,OAAAC,IAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAJ,WAAA;IACAC;EACA;EACAI,KAAA;IACAC,MAAA;MACAC,IAAA,EAAAC,OAAA;MACAC,OAAA;IACA;IACAC,QAAA;MACAH,IAAA,EAAAI,KAAA;MACAC,QAAA;IACA;IACAC,KAAA;MACAN,IAAA,EAAAO,MAAA;MACAL,OAAA;MACAM,UAAAC,GAAA;QACA,OAAAd,IAAA,CAAAe,KAAA,CAAAD,GAAA;MACA;IACA;IACAE,UAAA;MACAX,IAAA,EAAAY;IACA;IACAC,SAAA;MACAb,IAAA,EAAAI;IACA;EACA;EACAU,QAAA;IACAC,QAAA;MACA;IACA;IACAC,gBAAA;MACA,YAAAV,KAAA;IACA;EACA;EACAW,OAAA;IACAC,aAA